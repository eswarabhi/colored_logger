{
  "name": "ea-colored-log",
  "type": "flogo:activity",
  "ref": "github.com/eswarabhi/colored_logger",
  "version": "0.0.1",
  "title": "Colored Logger",
  "description": "Activity that prints colored logs",
  "homepage": "https://github.com/eswarabhi/colored_logger/tree/master",
  "input":[
    {
      "name": "message",
      "type": "string",
      "value": ""
    },
    {
      "name": "flowInfo",
      "type": "boolean",
      "value": false
    },
    {
      "name": "addToFlow",
      "type": "boolean",
      "value": false
    },
    {
      "name": "level",
      "type": "string",
      "required": true,
      "allowed" : ["Trace", "Debug", "Info", "Print", "Warn", "Error", "Fatal"]
    }
  ],
  "output": [
    {
      "name": "message",
      "type": "string"
    }
  ]
}
